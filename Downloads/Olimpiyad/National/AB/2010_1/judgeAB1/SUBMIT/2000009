#include <cstdio>
FILE *in; FILE *out;

int main(void)
{
	in = stdin; out = stdout;
//	in = fopen("crypto.in", "rt"); out = fopen("crypto.out", "wt");
	
	unsigned a, b, k;
	fscanf(in, "%u %u %u", &k, &a, &b);
	
	unsigned ans = 0;
	for (unsigned i = a; i <= b; i++)
	{
		unsigned cnt = 0;
		for (unsigned num = i; num > 0; num >>= 1)
		{
			cnt = (num & 1) ? cnt + 1 : 0;
			if (cnt >= k) {ans++; break;}
		}
	}
	fprintf(out, "%u\n", ans);
	return 0;
}
